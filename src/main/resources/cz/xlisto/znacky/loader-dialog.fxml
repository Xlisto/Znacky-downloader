<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane stylesheets="@/cz/xlisto/znacky/loader-dialog.css" xmlns:fx="http://javafx.com/fxml/1">
   <center>
       <VBox alignment="CENTER" maxHeight="150.0" maxWidth="150.0" minHeight="150.0" minWidth="150.0" prefHeight="150.0" prefWidth="150.0" spacing="10.0" styleClass="loader-dialog">
           <ProgressIndicator fx:id="progressIndicator" />
           <Label text="Načítám odkazy...">
            <padding>
                <Insets top="15.0" />
            </padding>
           </Label>
         <Label fx:id="progress"/>
       </VBox>
   </center>
</BorderPane>
